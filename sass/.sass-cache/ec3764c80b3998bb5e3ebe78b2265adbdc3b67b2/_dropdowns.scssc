// Tweak nav headers
// -----------------
// Increase padding from 15px to 20px on sides
.dropdown .dropdown-menu .nav-header {
  padding-right: 20px;
  padding-left: 20px;
}

// .dropdown .dropdown-menu .nav-header {
//   padding-left: 20px;
//   padding-right: 20px;
// }

// Typeahead
// ---------
.typeahead {
  margin-top: 2px;
  border-radius: 4px;
}

