// Link buttons
// --------------------------------------------------

.btn-link,
.btn-link:active,
.btn-link[disabled] {
  background-color: transparent;
  background-image: none;
  box-shadow: none;
}
.btn-link {
  border-color: transparent;
  border-radius: 0;
  color: #65619b;
  text-shadow: none;
  cursor: pointer;
}
.btn-link:hover {
  background-color: transparent;
  color: #53507c;
  text-decoration: underline;
  text-shadow: none;
}
.btn-link[disabled]:hover {
  color: #333333;
  text-decoration: none;
}


