// Groups of fields with labels on top (legends)
legend {
  display: block;
  margin-bottom: 20px;
  padding: 0;
  width: 100%;
  border: 0;
  border-bottom: 1px solid #B8B5A9;
  color: #333333;
  font-size: 21px;
  line-height: 40px;
}
legend small {
  color: #999999;
  font-size: 15px;
}

