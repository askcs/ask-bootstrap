.form-actions {
  margin-top: 20px;
  margin-bottom: 20px;
  padding: 19px 20px 20px;
  border-top: 1px solid #b8b5a9;
  background-color: #e6e4da;
  *zoom: 1;
}

.form-actions:before,
.form-actions:after {
  display: table;
  content: "";
  line-height: 0;
}
.form-actions:after {
  clear: both;
}




// HELP TEXT
// ---------

// .help-block,
// .help-inline {
//   color: lighten($textColor, 15%); // lighten the text some for contrast
// }

// .help-block {
//   display: block; // account for any element using help-block
//   margin-bottom: $baseLineHeight / 2;
// }

// .help-inline {
//   display: inline-block;
//   @include ie7-inline-block();
//   vertical-align: middle;
//   padding-left: 5px;
// }
.help-block,
.help-inline {
  color: #595959;
}
.help-block {
  display: block;
  margin-bottom: 10px;
}
.help-inline {
  display: inline-block;
  padding-left: 5px;
  vertical-align: middle;
  *display: inline;
  *zoom: 1;
}






