// Set font for forms
label,
input,
button,
select,
textarea {
  font-weight: normal;
  font-size: 14px;
  line-height: 20px;
}
input,
button,
select,
textarea {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}

