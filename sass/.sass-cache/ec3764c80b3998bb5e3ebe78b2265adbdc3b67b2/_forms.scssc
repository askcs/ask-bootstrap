// Form controls
// -------------------------

// Shared size and type resets
select,
textarea,
input[type="text"],
input[type="password"],
input[type="datetime"],
input[type="datetime-local"],
input[type="date"],
input[type="month"],
input[type="time"],
input[type="week"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"],
.uneditable-input {
  display: inline-block;
  margin-bottom: 10px;
  padding: 4px 6px;
  height: 20px;
  border-radius: 4px;
  color: #6D6149;
  vertical-align: middle;
  font-size: 14px;
  line-height: 20px;
}

